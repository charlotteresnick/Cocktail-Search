<div class="main-container">
  <div class="back-to-search">
    <a href="/search">&#8592; Back to Search</a>
  </div>
  <div class="search-results">
    <% locals.cocktails.forEach(cocktail => { %>
      <a class="cocktail" href="recipe/<%- cocktail.id %>">
        <h3><%-`${cocktail.name}` %></h3>
        <img class="cocktail-img" src="<%- cocktail.image %>" alt="cocktail image">
      </a>
    <% }) %>
  </div>
</div>